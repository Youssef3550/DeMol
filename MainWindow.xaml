        <TextBox x:Name="TxtNaam" HorizontalAlignment="Left" Height="22" Margin="577,176,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="166"/>
        <Button x:Name="BtnAanmelden" Content="Aanmelden" HorizontalAlignment="Left" Height="21" Margin="577,332,0,0" VerticalAlignment="Top" Width="166" Click="BtnAanmelden_Click"/>

    </Grid>
